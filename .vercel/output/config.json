{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_render"
		},
		{
			"src": "^\\/artikel\\/author\\/([^/]+?)\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/artikel\\/author\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/artikel\\/category\\/([^/]+?)\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/artikel\\/category\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/artikel\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/beasiswa\\/page\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "^\\/beasiswa\\/([^/]+?)\\/?$",
			"dest": "_render"
		},
		{
			"src": "/.*",
			"dest": "/404.html",
			"status": 404
		}
	]
}